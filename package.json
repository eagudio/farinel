{
  "name": "farinel",
  "version": "0.1.2",
  "description": "A clever and mischievous framework that makes your code dance with joy! As cunning as it is lightweight - because being smart doesn't mean being heavy!",
  "main": "lib/main.js",
  "types": "lib/main.d.ts",
  "exports": {
    ".": {
      "types": "./lib/main.d.ts",
      "import": "./lib/main.js"
    },
    "./html": {
      "types": "./lib/html/index.d.ts",
      "import": "./lib/html/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "prepublishOnly": "npm run build",
    "test": "jest"
  },
  "files": [
    "lib/**/*",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/eagudio/farinel.git"
  },
  "keywords": [
    "framework",
    "html",
    "dom",
    "ui",
    "lightweight",
    "chainable",
    "typescript",
    "functional",
    "fluent",
    "elegant",
    "minimal",
    "smart",
    "developer-tools",
    "ciaplu"
  ],
  "license": "MIT",
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@types/jest": "^29.5.14",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "ts-jest": "^29.2.6",
    "ts-node": "^10.9.2"
  },
  "dependencies": {
    "ciaplu": "^2.4.1"
  }
}
